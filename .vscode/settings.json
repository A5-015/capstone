{
    "beautify.onSave": true,
    "editor.formatOnSave": true,
    "editor.formatOnType": true,
    "clang-tidy.lintOnSave": true,
    "files.associations": {
        "functional": "cpp",
        "unordered_map": "cpp",
        "tuple": "cpp",
        "vector": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "*.tcc": "cpp",
        "bitset": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "exception": "cpp",
        "fstream": "cpp",
        "future": "cpp",
        "initializer_list": "cpp",
        "iomanip": "cpp",
        "iosfwd": "cpp",
        "iostream": "cpp",
        "istream": "cpp",
        "limits": "cpp",
        "mutex": "cpp",
        "new": "cpp",
        "ostream": "cpp",
        "numeric": "cpp",
        "ratio": "cpp",
        "sstream": "cpp",
        "stdexcept": "cpp",
        "streambuf": "cpp",
        "system_error": "cpp",
        "thread": "cpp",
        "cinttypes": "cpp",
        "regex": "cpp",
        "type_traits": "cpp",
        "utility": "cpp",
        "typeinfo": "cpp"
    },
    "cmake.configureOnOpen": false,
    "autoDocstring.docstringFormat": "sphinx",
    "autoDocstring.startOnNewLine": true,
    "extensions.ignoreRecommendations": false,
    "editor.rulers": [
        80
    ],
    "python.sortImports.args": [
        "--trailing-comma",
        "--force-grid-wrap",
        "0",
        "--use-parentheses",
        "--line-width",
        "80",
        "--multi-line",
        "3",
        "--project",
        "ramen"
    ],
    "python.formatting.provider": "black",
    "python.formatting.blackArgs": [
        "--line-length=80"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    },
    "python.linting.pylintEnabled": true,
    "python.linting.pylintArgs": [
        "--enable=W0611"
    ],
    "[cpp]": {
        "editor.defaultFormatter": "ms-vscode.cpptools"
    }
}